class Solution {
    int isPlaindrome(String S) {
        // code here
        int ans=1;
        int i=0,j=S.length()-1;
        while(i<j)
        {
            if(S.charAt(i)!=S.charAt(j))
            {
                ans=0;
                break;
            }
            i++;
            j--;
        }
        return ans;
    }
};
